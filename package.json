{
  "name": "yauzl-mac",
  "version": "2.0.0",
  "description": "Unzipping with yauzl with added support for Mac OS Archive Utility ZIP files",
  "main": "index.js",
  "files": [
    "lib/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/yauzl-mac.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/yauzl-mac/issues"
  },
  "dependencies": {
    "@overlookmotel/yauzl": "^3.1.0",
    "yauzl-clone": "^2.0.0"
  },
  "devDependencies": {
    "@overlookmotel/eslint-config": "^10.1.0",
    "@overlookmotel/eslint-config-jest": "^6.0.1",
    "@overlookmotel/eslint-config-node": "^4.1.0",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-runner-eslint": "^2.0.0",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "yauzl",
    "unzip",
    "zip",
    "mac",
    "mac os",
    "macos",
    "os x",
    "osx",
    "archive utility"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "jest --config=jest-eslint.config.js",
    "lint-fix": "eslint . --fix",
    "test-main": "jest",
    "cover": "npm-run-all -c cover-main cover-cleanup",
    "cover-main": "jest --coverage",
    "cover-cleanup": "rm -rf coverage"
  },
  "engines": {
    "node": ">=16"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
